  | select(.value.address? and (.value.address | test("^0x[0-9a-fA-F]{40}$")))
  | {
      address: (.value.address | ascii_downcase),
      symbol: (.value.symbol // ""),
      name: (.value.name // ""),
      decimals: (.value.decimals // ""),
      chainId: (
        if (.value.iconUrl? // "" | test("tokenIcons/[0-9]+/")) then
          (.value.iconUrl | capture("tokenIcons/(?<cid>[0-9]+)/").cid)
        else "" end
      ),
      aggregators: (.value.aggregators // [])
    }
  | [.address, .symbol, .name, .chainId, .decimals, (.aggregators | join("|"))]
  | @csv
' MetaMask_state.json > token_contracts_combined.csv
sha256sum token_contracts_combined.csv >> CERT_METAMASK_STATE_0X_SWARM.sha256
find ~/ -type f -size 0 -print > empty_files.txt
cat empty_files.txt | xargs -I{} echo "rm '{}'" > purge_script.sh
bash purge_script.sh
find ~/ -type f -name "*.json" -exec jq empty {} \; -exec echo {} \; 2>&1 | grep -B1 "parse error" > broken_json.txt
cat broken_json.txt | grep '^/' | xargs -I{} echo "rm '{}'" >> purge_script.sh
bash purge_script.sh
